config 'global'
    option  anon_swap       '0'
    option  anon_mount      '0'
    option  auto_swap       '0'
    option  auto_mount      '1'
    option  check_fs        '0'
    option  delay_root      '0'

config 'mount'
    option 'target'         '/vfat0'
    option 'options'        'rw,noatime,nodiratime,exec'
    option 'device'         '/dev/mmcblk0p1'
    option 'enabled'        '1'
    option 'fstype'         'vfat'
    option 'enabled_fsck'   '0'
    option 'is_rootfs'      '0'

